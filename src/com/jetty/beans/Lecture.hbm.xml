<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jetty.beans">
    <class name="Lecture" table="lecture">

        <id name="id" type="java.lang.Integer"><column name="id" />
            <generator class="native"></generator>
        </id>

        <property name="pageid" type="java.lang.Integer"><column name="pageid" /></property>
        <property name="lecturetitle" column="lecturetitle" type="java.lang.String"
                  length="40"></property>
        <property name="lecturename" column="lecturename" type="java.lang.String"
                  length="40"></property>

    </class>
</hibernate-mapping>